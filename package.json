{
  "name": "electron-alarm-clock",
  "version": "1.0.0",
  "description": "Sample app for Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . AlarmClock --out AlarmClock --overwrite --platform=win32 --arch=x64 --icon=clock.ico --prune=true --squirrel-install --ignore=node_modules/electron-* --electron-version=11.1.1 --ignore=AlarmClock-win32-x64 --version-string.CompanyName=Robby --version-string.ProductName=AlarmClock",
    "pack": "grunt"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/explooosion/electron-alarm-clock"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/explooosion/electron-alarm-clock/issues"
  },
  "homepage": "https://github.com/explooosion/electron-alarm-clock#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "electron": "^11.1.1",
    "electron-packager": "^15.2.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-formatting": "0.0.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^4.11.0",
    "grunt": "^1.3.0",
    "grunt-electron-installer": "^2.1.0"
  },
  "dependencies": {
    "moment": "^2.22.2",
    "node-notifier": "^5.4.3"
  }
}
